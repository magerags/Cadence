<%= render 'shared/navbar' %>
<div class="page-content p-5" id="content">
<%= simple_form_for @event do |f| %>
<%= f.association :category %>
<%= f.input :name %>
<%= f.input :description%>
<%= f.input :starting_time%>
<%= f.input :ending_time%>
<%= f.submit %>
<% end %>
<p><%= link_to "Back", events_path %></p>
</div>
